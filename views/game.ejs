		<script>
			//window.onload=init;
		</script>
  
	</head>
	
	<body onload="init();">
		<div class="container">

			<div class="row-fluid">
				
					<div class="game">
						<canvas id="info" width="200" height="390"></canvas>
						<canvas id="game" width="510" height="390" tabindex="1"><a href="https://www.google.com/intl/fr/chrome/browser/?hl=fr"><img src="/images/incompatible.png"></a></canvas>
					</div>
				
			</div>
			
			<% //if( !isPublic ){ %>
			<br>
			<div class="row-fluid">
				<div class="offset1">
					<input class="span2" id="invitePseudo" size="16" type="text" placeholder="Son pseudo">
				</div>
			</div>
			<div class="row-fluid">
				<div class="offset1">
					<button id="btnInvitation" type="button" class="btn btn-large" style="width: 128px;">Inviter un ami</button>
				</div>
			</div>
			<% //} %>


		</div> <!-- /container -->
		<span id="hidden-pseudo" style="display: none;"><%= pseudo %></span>
		<span id="hidden-gamename" style="display: none;"><%= gamename %></span>
	</body>
</html>
